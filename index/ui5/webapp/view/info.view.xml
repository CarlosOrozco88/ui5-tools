<mvc:View xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    displayBlock="true"
    width="100%"
    height="100%">
    <Page title="Info">
        <f:SimpleForm>
            <f:content>
                <Label text="Resources proxy" />
                <Text text="{ui5tools>/config/resourcesProxy}" />
                <Label text="UI5 Version" />
                <Text text="{ui5tools>/config/ui5Version}" />
                <Label text="OData proxy" />
                <Text text="{ui5tools>/config/gatewayProxy}" />
                <Label text="Gateway URL" />
                <Text text="{ui5tools>/config/gatewayUri}" />
            </f:content>
        </f:SimpleForm>
    </Page>
</mvc:View>

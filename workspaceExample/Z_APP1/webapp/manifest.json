{
	"_version": "1.12.0",
	"sap.app": {
		"id": "example.Z_APP1",
		"type": "application",
    "dataSources": {
      "northwind": {
        "uri": "/odata/V2/Northwind/Northwind.svc/",
        "type": "OData",
        "settings": {
					"odataVersion": "2.0",
					"mode": "TwoWay"
        }
      }
    }
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.71.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"rootView": {
			"viewName": "example.Z_APP1.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"models": {
      "northwind": {
        "dataSource": "northwind"
      },
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "example.Z_APP1.i18n.i18n",
					"supportedLocales": ["en"],
					"fallbackLocale": "en"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			]
		}
	}
}
